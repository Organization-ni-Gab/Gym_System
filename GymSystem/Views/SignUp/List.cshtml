@model IEnumerable<Signup>

@{
	ViewData["Title"] = "SignUp";
}
<h2>Sign-up List</h2>
<a asp-controller="Signup" asp-action="Create" class="btn btn-primary ">Add Client</a>

<table class="table table-bordered text-center mt-3">
	<thead>
		<tr>
			<th>First Name</th>
			<th>Middle Name</th>
			<th>Last Name</th>
			<th>Contact Number</th>
			<th>Gender</th>
			<th>Action</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var customer in Model)
		{
			<tr>
				<td>@customer.FirstName</td>
				<td>@customer.MiddleName</td>
				<td>@customer.LastName</td>
				<td>@customer.ContactNumber</td>
				<td>@customer.Gender</td>
				@* <td>
					<div></div>
				</td>  CREATE AJAX ACTION FOR EDIT,WALKIN,MEMBERSHIP*@
			</tr>
		}
	</tbody>
</table>
